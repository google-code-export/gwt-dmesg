<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module>

	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<inherits name="com.google.gwt.i18n.I18N" />
	<inherits name="org.gwt.dmesg.DynamicMessages" />
	
	<extend-configuration-property name="messageBundles" value="Messages"/>
	<extend-configuration-property name="messageBundles" value="OtherBundle"/>
</module>
