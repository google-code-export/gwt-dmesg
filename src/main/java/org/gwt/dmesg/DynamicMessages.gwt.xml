<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module>
	<define-configuration-property name="messageBundles" is-multi-valued="true" />

	<generate-with class="org.gwt.dmesg.rebind.DynamicMessagesGenerator">
		<when-type-is class="org.gwt.dmesg.client.MessageBundleImpl" />
	</generate-with>
</module>
